FrameWidgetClass rootFrame {
 size 1 32
 halign center_ref
 hexactpos 1
 vexactpos 1
 hexactsize 0
 vexactsize 1
 {
  GridSpacerWidgetClass Grid {
   ignorepointer 1
   position 0 0
   size 1 1
   valign center_ref
   hexactpos 1
   vexactpos 1
   hexactsize 0
   vexactsize 0
   Padding 16
   Margin 0
   "Size To Content H" 1
   Columns 4
   Rows 1
   {
    TextWidgetClass PlayerNameLabel {
     ignorepointer 1
     size 400 1
     halign center_ref
     valign center_ref
     hexactpos 1
     vexactpos 1
     hexactsize 1
     vexactsize 0
     text "Player Name"
     font "gui/fonts/sdf_MetronBook72"
     "bold text" 1
     "exact text" 1
     "exact text size" 20
     "size to text h" 0
     "text valign" center
    }
    TextWidgetClass PlayerKillsLabel {
     ignorepointer 1
     size 144 1
     halign center_ref
     valign center_ref
     hexactpos 1
     vexactpos 1
     hexactsize 1
     vexactsize 0
     text "KILLS"
     font "gui/fonts/sdf_MetronBook72"
     "bold text" 1
     "exact text" 1
     "exact text size" 20
     "text halign" center
     "text valign" center
    }
    TextWidgetClass ZombieKillsLabel {
     ignorepointer 1
     position 10 0
     size 144 1
     hexactpos 1
     vexactpos 1
     hexactsize 1
     vexactsize 0
     text "ZOMBIE KILLS"
     font "gui/fonts/sdf_MetronBook72"
     "bold text" 1
     "exact text" 1
     "exact text size" 20
     "text halign" center
     "text valign" center
    }
    TextWidgetClass SurvivalTimeLabel {
     visible 0
     ignorepointer 1
     position 10 10
     size 160 48
     hexactpos 1
     vexactpos 1
     hexactsize 1
     vexactsize 1
     text "SURVIVAL TIME"
     font "gui/fonts/sdf_MetronBook72"
     "bold text" 1
     "exact text" 1
     "exact text size" 32
     "text halign" center
    }
   }
  }
 }
}